    "def sum_of_natural_numbers(n):\n",
    "    total = 0\n",
    "    for i in range(1, n + 1):\n",
    "        total += i\n",
    "    return total\n",
    "\n",
    "n = int(input(\"Enter the value of 'n': \"))\n",
    "sum_of_n = sum_of_natural_numbers(n)\n",
    "print(f\"The sum of the first {n} natural numbers is: {sum_of_n}\")"
